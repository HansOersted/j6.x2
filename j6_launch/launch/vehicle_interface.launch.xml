<?xml version="1.0" encoding="UTF-8"?>
<launch>

 <arg name="can_device" default="can0"/>
  <arg name="vehicle_id" default="$(env VEHICLE_ID default)"/>

  <!-- j6_interface -->
  <include file="$(find-pkg-share j6_interface)/launch/j6_interface.launch.xml">
    <arg name="can_device" value="$(var can_device)"/>
    <arg name="vehicle_calibration_param_path"  value="$(find-pkg-share individual_params)/config/$(var vehicle_id)/vehicle/vehicle_calibration.param.yaml"/>
  </include>

</launch>
